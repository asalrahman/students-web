<%- include('layouts/header') %>

<div class="container mt-5 ">
    <div class="row mt-5">
        <div class="col-lg-11">
            <div class="tale-responsive ">
                
                <table class="table ">
                    <thead>
                      <tr class="table-dark">
                        <th scope="col">NO</th>
                        <th scope="col">Name</th>
                        <th scope="col">Age</th>
                        <th scope="col">Course</th>
                        <th scope="col">Year Of Passout</th>
                        <th scope="col">Remove/Delete</th>
                      </tr>
                    </thead>
                    <tbody>
                     <% users.forEach((row,index)=> { %>
                        <tr>
                            <td><%= index+1 %></td>
                            <td><%= row.name %></td>
                            <td><%= row.age %></td>
                            <td><%= row.course %></td>
                            <td><%= row.passoutyear %></td>
                            <td><a href="/edit/<%= row._id %>" class="text-success"><i class="fas fa-edit fa-lg mx-1"></i></a>
                             <a href="/delete/<%= row._id %>" class="text-danger"><i class="fas fa-trash fa-lg mx-1"></i></a>
                              </td>
                          </tr>
                    <% }) %>
                     
                    </tbody>
                  </table>
            </div>
        </div>
    </div> 
</div>

<%- include('layouts/footer') %>


